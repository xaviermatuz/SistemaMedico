@section CrearExamenFisicoScript
{

    <script src="~/Recursos/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>
    <!-- Input Mask Plugin Js -->
    <script src="~/Recursos/plugins/jquery-inputmask/jquery.inputmask.bundle.js"></script>
    <script>
        $('.btnNext').click(function () {
            $('.nav-tabs > .active').next('li').find('a').trigger('click');
        });
        $('.btnPrevious').click(function () {
            $('.nav-tabs > .active').prev('li').find('a').trigger('click');
        });
    </script>
    <script>
        $(function () {
            $('#addfilaNE').on('click', function () {
                var data = $("#tb tr:eq(1)").clone(true).appendTo("#tb");
                data.find("input").val('');
            });
            $(document).on('click', '.remove', function () {
                var trIndex = $(this).closest("tr").index();
                if (trIndex > 1) {
                    $(this).closest("tr").remove();
                } else {
                    alert("lo siento no se puede eliminar la primera fila ");
                }
            });
        });
    </script>
    <script>
        $(document).ready(function () {

            var now = new Date();

            var day = ("0" + now.getDate()).slice(-2);
            var month = ("0" + (now.getMonth() + 1)).slice(-2);

            var today = (day) + "/" + (month) + "/" + now.getFullYear();
            $("#fecha").text = today;
            $("#fecha").val(today);
            console.log(today);
        });
    </script>
    <script>
        $(function () {
            var $demoMaskedInput = $('.demo-masked-input');
            $demoMaskedInput.find('.date').inputmask('dd/mm/yyyy', { placeholder: '__/__/____' });
        });
        // $demoMaskedInput.find('.date').inputmask('dd/mm/yyyy', { placeholder: '__/__/____' });
        $(function () {
            var $demoMaskedInput = $('.demo-masked-input');
            $demoMaskedInput.find('.cedula').inputmask('[0-3]/mm/yyyy', { placeholder: '__/__/____' });
        });
    </script>
    <script type="text/javascript">
        function Habilitar_DesabilitarTXT(chkSI, txtID) {
            txtID.disabled = chkSI.checked ? true : false;
            if (chkSI.checked == true) {
                if (txtID.value != "") {
                    txtID.value = "";
                }
            }
        }
    </script>
}   